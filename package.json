{
  "name": "effect-schema-geojson",
  "version": "0.1.0",
  "private": false,
  "description": "Effect Schema for GeoJSON types - comprehensive validation and parsing library",
  "keywords": [
    "effect",
    "schema",
    "geojson",
    "validation",
    "typescript",
    "gis",
    "geometry",
    "spatial",
    "maps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tvshevchuk/effect-schema-geojson.git"
  },
  "homepage": "https://github.com/tvshevchuk/effect-schema-geojson#readme",
  "bugs": {
    "url": "https://github.com/tvshevchuk/effect-schema-geojson/issues"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "npx tsx tests/basic.test.ts",
    "example": "npx tsx examples/basic.ts",
    "prepublishOnly": "npm run build && npm run test",
    "deploy": "./scripts/deploy.sh",
    "deploy:github": "git push -u origin main",
    "deploy:npm": "npm publish"
  },
  "peerDependencies": {
    "effect": ">=3.10.0"
  },
  "devDependencies": {
    "effect": "^3.10.0",
    "tsdown": "latest",
    "@types/geojson": "^7946.0.14",
    "@types/node": "24.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
